within solarGreenhouseV2.Interfaces;
model fixedPAR "simplePAR source"
  parameter Modelica.Units.SI.HeatFlux magPAR "magnitude of par heat flux";
  PAR_output I_par annotation (Placement(transformation(extent={{-22,-14},
            {18,26}})));
equation
  I_par = magPAR;

  annotation (Icon(coordinateSystem(preserveAspectRatio=false)),
      Diagram(coordinateSystem(preserveAspectRatio=false)));
end fixedPAR;
